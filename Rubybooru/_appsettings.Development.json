{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "RubybooruDb": "Data Source=./rubybooru.db"
  },
  "LogQueries": false,
  "ImagesPath": "C:/Images",
  "DefaultPlaceholder": "default.jpg",
  "ClientBuildPath": "C:/Client",
  "DuplicateCheckerMaxRMS": 3,
  "IqdbCheckIntervalInDays": 28,
  "IqdbRequestDelayInMs": 750,
  "IqdbMinSimilarity": 80,
  "AllowedPreviewOptions": {
    "Options": [
      {
        "Width": 350,
        "Height": 180,
        "KeepRatio": true
      }
    ]
  }
}
